<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>socialgraph</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-css>/js/jquery-loadmask/jquery.loadmask.css</header-portlet-css>
		<header-portlet-css>/js/bootstrap/css/bootstrap.css</header-portlet-css>
        <header-portlet-css>/js/bootstrap/css/bootstrap-responsive.css</header-portlet-css>
		<header-portlet-javascript>http://www.google.com/jsapi</header-portlet-javascript>
		<header-portlet-javascript>/js/bootstrap/js/bootstrap.js</header-portlet-javascript>
		<header-portlet-javascript>/js/network-1.5/network.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery-loadmask/jquery.loadmask.js</header-portlet-javascript>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>		
		<css-class-wrapper>SocialNeworks-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>login</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>false</instanceable>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>login-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>